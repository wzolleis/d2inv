    "mini-css-extract-plugin": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "openapi-client-axios-typegen": "^3.14.1",
    "path": "^0.12.7",
    "postcss-loader": "^6.1.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.3",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.44.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "comments": {
    "sass-loader": "11.x funktioniert nicht, siehe https://exerror.com/typeerror-this-getoptions-is-not-a-function-in-vue-js/",
    "node-sass-6.x": "passt zu node 16.x"
  },
  "scripts": {
    "build": "npm run build:prod",
    "copy:files": "copyfiles -u 1 dist/**/* ../divinv-backend/build/resources/main/public/ui-static",
    "prebuild": "npm run typegen-api",
    "postbuild": "npm run copy:files",
    "eject": "react-scripts eject",
    "typegen-api": "typegen ../specs/divinv_api.yaml > ./src/api/apiTypes.d.ts",
    "typegen-pets": "typegen ../specs/pets.yaml > ./src/api/pets.d.ts",
    "build:dev": "webpack --mode development",
    "build:prod": "webpack --mode production",
    "test": "jest",
    "start": "webpack serve --mode development --open"
  },
  "proxy": "http://localhost:8080",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
