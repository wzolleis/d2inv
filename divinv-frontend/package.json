{
  "name": "divinv-client",
  "version": "0.1.0",
  "private": true,
  "homepage": "/ui-static",
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "5.0.2",
    "bootstrap-icons": "^1.5.0",
    "jquery": "^3.6.0",
    "openapi-client-axios": "^4.0.0",
    "popper.js": "^1.16.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.13.0",
    "react-redux": "^7.2.4",
