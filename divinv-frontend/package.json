{
  "name": "divinv-client",
  "version": "0.1.0",
  "private": true,
  "homepage": "/ui-static",
  "dependencies": {
    "axios": "^0.21.1",
    "bootstrap": "5.0.1",
    "bootstrap-icons": "^1.5.0",
    "jquery": "^3.6.0",
    "openapi-client-axios": "^3.14.0",
    "popper.js": "^1.16.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0",
    "typescript": "^4.3.4",
    "typescript-fsa": "^3.0.0",
    "web-vitals": "^2.0.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.12.4",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-redux": "^7.1.16",
    "@types/react-router-dom": "^5.1.7",
    "copyfiles": "^2.4.1",
    "css-loader": "^5.2.6",
    "mini-css-extract-plugin": "^1.6.2",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "openapi-client-axios-typegen": "^3.14.1",
    "path": "^0.12.7",
    "sass-loader": "^12.1.0",
    "style-loader": "^2.0.0"
  },
  "comments": {
    "sass-loader": "11.x funktioniert nicht, siehe https://exerror.com/typeerror-this-getoptions-is-not-a-function-in-vue-js/",
    "node-sass-6.x": "passt zu node 16.x"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "copy:files": "copyfiles -u 1 build/**/* ../divinv-backend/build/resources/main/public/ui-static",
    "prebuild": "npm run typegen-api",
    "postbuild": "npm run copy:files",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "typegen-api": "typegen ../specs/divinv_api.yaml > ./src/api/apiTypes.d.ts",
    "typegen-pets": "typegen ../specs/pets.yaml > ./src/api/pets.d.ts"
  },
  "proxy": "http://localhost:8080",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
